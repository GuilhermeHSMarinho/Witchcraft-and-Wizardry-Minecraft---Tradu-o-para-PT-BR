{
    "multipart": [
        {   "apply": { "model": "block/fence/oak_post" }},
        {   "when": { "north": "true" },
            "apply": { "model": "block/fence/oak_side" }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/fence/oak_side", "y": 90}
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/fence/oak_side", "y": 180}
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/fence/oak_side", "y": 270}
        }
    ]
}
